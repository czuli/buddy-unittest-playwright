{
  "name": "playwright-test-project",
  "version": "1.0.0",
  "description": "Prosty projekt do testowania Playwright",
  "main": "index.html",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "serve": "npx http-server . -p 3000",
    "install:browsers": "playwright install"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "automation"
  ],
  "author": "Test User",
  "license": "MIT",
  "devDependencies": {
    "@buddy-works/unit-tests": "^0.20.0",
    "@playwright/test": "^1.57.0",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=16"
  }
}
